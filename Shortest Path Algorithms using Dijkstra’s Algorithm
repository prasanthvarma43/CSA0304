 #include <stdio.h>
#define V 5
#define INF 999
int g[V][V]={{0,10,0,5,0},{0,0,1,2,0},{0,0,0,0,4},{0,3,9,0,2},{7,0,6,0,0}},d[V],vis[V];
int main(){
    for(int i=0;i<V;i++){d[i]=INF;vis[i]=0;} d[0]=0;
    for(int c=0;c<V;c++){int u=-1,min=INF;
        for(int i=0;i<V;i++) if(!vis[i]&&d[i]<min){min=d[i];u=i;}
        vis[u]=1;
        for(int v=0;v<V;v++) if(g[u][v]&&d[u]+g[u][v]<d[v]) d[v]=d[u]+g[u][v];
    }
    for(int i=0;i<V;i++) printf("0->%d=%d\n",i,d[i]);
}
