#include <stdio.h>

int main() {
    int nums[] = {12, 7, 5, 8, 13};
    int size = sizeof(nums)/sizeof(nums[0]);

    for (int i = 0; i < size; i++) {
        if (nums[i] % 2 == 0)
            printf("%d is Even\n", nums[i]);
        else
            printf("%d is Odd\n", nums[i]);
    }
    return 0;
}
