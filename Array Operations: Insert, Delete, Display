#include <stdio.h>

int main() {
    int arr[100] = {10, 20, 30, 40, 50};
    int n = 5;
    printf("Original Array:\n");
    for (int i = 0; i < n; i++)
        printf("%d ", arr[i]);
    int pos = 2, val = 25;
    for (int i = n; i > pos; i--)
        arr[i] = arr[i - 1];
    arr[pos] = val;
    n++;
    printf("\nAfter Insertion (25 at index 2):\n");
    for (int i = 0; i < n; i++)
        printf("%d ", arr[i]);
 for (int i = 3; i < n - 1; i++)
        arr[i] = arr[i + 1];
    n--;
    printf("\nAfter Deletion (index 3):\n");
    for (int i = 0; i < n; i++)
        printf("%d ", arr[i]);
    return 0;
}
