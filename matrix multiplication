#include <stdio.h>

int main() {
  int a[2][2]={2,4,6,8};
  int b[2][2]={12,13,25,17};
  int c[2][2]={0,0,0,0};
  int i,j;
  for(i=0;i<2;i++)
  {
      for(j=0;j<2;j++)
      {
          c[i][j]=a[i][j]*b[i][j];
      }
  }
  for(i=0;i<2;i++)
  {
      for(j=0;j<2;j++)
      {
          printf("%d\t",c[i][j]);
      }
      printf("\n");
  }

    return 0;
}
